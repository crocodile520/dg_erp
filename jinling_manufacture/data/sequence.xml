<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data noupdate="1">
        <record id="jinling_manufacture_mes_plm" model="ir.sequence">
            <field name="name">生产工单</field>
            <field name="code">jl.mes.plm</field>
            <field eval="6" name="padding"/>
            <field name="prefix">JMP%(y)s%(month)s</field>
        </record>

         <record id="jinling_manufacture_mes_plm_in" model="ir.sequence">
            <field name="name">生产入库单</field>
            <field name="code">jl.mes.plm.in</field>
            <field eval="6" name="padding"/>
            <field name="prefix">JMPI%(y)s%(month)s</field>
        </record>

        <record id="jinling_manufacture_mes_plm_picking_in" model="ir.sequence">
            <field name="name">生产领料单</field>
            <field name="code">jl.mes.plm.picking</field>
            <field eval="6" name="padding"/>
            <field name="prefix">JMPP%(y)s%(month)s</field>
        </record>

        <record id="jinling_manufacture_mes_plm_refund_out" model="ir.sequence">
            <field name="name">生产退料单</field>
            <field name="code">jl.mes.plm.refund</field>
            <field eval="6" name="padding"/>
            <field name="prefix">JMPR%(y)s%(month)s</field>
        </record>
    </data>
</odoo>