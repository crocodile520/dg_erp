<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data noupdate="1">
        <record id="jinling_sample_apply" model="ir.sequence">
            <field name="name">样品申请单</field>
            <field name="code">jl.sample.apply</field>
            <field eval="6" name="padding"/>
            <field name="prefix">JSA%(y)s%(month)s</field>
        </record>

         <record id="jinling_sample_order" model="ir.sequence">
            <field name="name">样品订单</field>
            <field name="code">jl.sample.order</field>
            <field eval="6" name="padding"/>
            <field name="prefix">JSO%(y)s%(month)s</field>
        </record>

        <record id="jinling_sample_picking" model="ir.sequence">
            <field name="name">样品领料单</field>
            <field name="code">jl.sample.picking</field>
            <field eval="6" name="padding"/>
            <field name="prefix">JSP%(y)s%(month)s</field>
        </record>

        <record id="jinling_sample_in" model="ir.sequence">
            <field name="name">样品入库单</field>
            <field name="code">jl.sample.in</field>
            <field eval="6" name="padding"/>
            <field name="prefix">JSI%(y)s%(month)s</field>
        </record>
    </data>
</odoo>