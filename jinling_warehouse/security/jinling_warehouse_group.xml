<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record model="ir.module.category" id="jinling_warehouse.charge_category">
        <field name="name">仓库</field>
        <field name="sequence">10</field>
    </record>

    <record id="jinling_warehouse_groups" model="res.groups">
        <field name="name">仓库</field>
        <field name="category_id" ref="jinling_warehouse.charge_category"/>
    </record>

    <record id="jinling_move_in_done_groups" model="res.groups">
        <field name="name">额外入库单确认</field>
        <field name="category_id" ref="jinling_warehouse.charge_category"/>
    </record>

    <record id="jinling_move_in_draft_groups" model="res.groups">
        <field name="name">额外入库单撤销</field>
        <field name="category_id" ref="jinling_warehouse.charge_category"/>
    </record>

    <record id="jinling_move_out_done_groups" model="res.groups">
        <field name="name">额外出库单确认</field>
        <field name="category_id" ref="jinling_warehouse.charge_category"/>
    </record>

    <record id="jinling_move_out_draft_groups" model="res.groups">
        <field name="name">额外出库单撤销</field>
        <field name="category_id" ref="jinling_warehouse.charge_category"/>
    </record>

</odoo>