<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data noupdate="1">
        <record id="jinling_move" model="ir.sequence">
            <field name="name">移库单</field>
            <field name="code">jl.move</field>
            <field eval="6" name="padding"/>
            <field name="prefix">JM%(y)s%(month)s</field>
        </record>

        <record id="jinling_move_in" model="ir.sequence">
            <field name="name">额外入库单</field>
            <field name="code">jl.move.in</field>
            <field eval="6" name="padding"/>
            <field name="prefix">JMI%(y)s%(month)s</field>
        </record>

        <record id="jinling_move_out" model="ir.sequence">
            <field name="name">额外出库单</field>
            <field name="code">jl.move.in</field>
            <field eval="6" name="padding"/>
            <field name="prefix">JMO%(y)s%(month)s</field>
        </record>
    </data>
</odoo>