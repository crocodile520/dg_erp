<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data noupdate="1">
        <record id="jinling_order_sequence" model="ir.sequence">
            <field name="name">销售订单</field>
            <field name="code">sell.order</field>
            <field eval="6" name="padding"/>
            <field name="prefix">SO%(y)s%(month)s</field>
        </record>

        <record id="jinling_apply_sequence" model="ir.sequence">
            <field name="name">销售申请单</field>
            <field name="code">sell.apply</field>
            <field eval="6" name="padding"/>
            <field name="prefix">SA%(y)s%(month)s</field>
        </record>

        <record id="jinling_sell_order_out_sequence" model="ir.sequence">
            <field name="name">销售发货单</field>
            <field name="code">sell.order.out</field>
            <field eval="6" name="padding"/>
            <field name="prefix">SOO%(y)s%(month)s</field>
        </record>

        <record id="jinling_sell_order_review_sequence" model="ir.sequence">
            <field name="name">销售评审单</field>
            <field name="code">sell.order.review</field>
            <field eval="6" name="padding"/>
            <field name="prefix">SOR%(y)s%(month)s</field>
        </record>
    </data>
</odoo>