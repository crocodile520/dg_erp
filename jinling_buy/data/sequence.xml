<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data noupdate="1">
        <record id="jinling_buy_apply" model="ir.sequence">
            <field name="name">采购申请单</field>
            <field name="code">jl.buy.apply</field>
            <field eval="6" name="padding"/>
            <field name="prefix">BA%(y)s%(month)s</field>
        </record>
        <record id="jinling_buy_order" model="ir.sequence">
            <field name="name">采购订单</field>
            <field name="code">jl.buy.order</field>
            <field eval="6" name="padding"/>
            <field name="prefix">BO%(y)s%(month)s</field>
        </record>
        <record id="jinling_buy_warehousing" model="ir.sequence">
            <field name="name">采购入库订单</field>
            <field name="code">jl.buy.warehousing</field>
            <field eval="6" name="padding"/>
            <field name="prefix">BW%(y)s%(month)s</field>
        </record>
        <record id="jinling_buy_return" model="ir.sequence">
            <field name="name">退货单</field>
            <field name="code">jl.buy.return</field>
            <field eval="6" name="padding"/>
            <field name="prefix">BR%(y)s%(month)s</field>
        </record>
        <record id="jinling_buy_payment_apply" model="ir.sequence">
            <field name="name">付款申请单</field>
            <field name="code">jl.buy.payment.apply</field>
            <field eval="6" name="padding"/>
            <field name="prefix">BPA%(y)s%(month)s</field>
        </record>
    </data>
</odoo>